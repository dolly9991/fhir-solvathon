map "http://jembi.org/fhir/StructureMap/CivilRegistrationToFHIRPatient" = "CivilRegistrationToFHIRPatient"

uses "http://jembi.org/fhir/StructureDefinition/CivilRegistrationLogicalModel" as source
uses "http://hl7.org/fhir/StructureDefinition/Patient" as target

group CivilToFHIR(source src : CivilPatient, target tgt : FHIRPatient) {
  src.name as srcName -> tgt.name as pName then {
    srcName -> pName.text "SetPatientFullName";
  };
}
